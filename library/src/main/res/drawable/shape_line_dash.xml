<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="line">
    <!--线宽为dashWith，线之间空隙dashGap，dashGap=0dp时，是实线 -->
    <stroke
        android:width="4dp"
        android:color="#cecece"
        android:dashWidth="4dp"
        android:dashGap="4dp" />
</shape>